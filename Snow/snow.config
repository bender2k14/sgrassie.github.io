{
  "author" : "Stuart Grassie",
  "email" : "sgrassie@gmail.com",
  "blogTitle" : "temporalcohesion.co.uk",
  "siteUrl": "http://www.temporalcohesion.co.uk",
  "posts": "_posts",
  "layouts": "_layouts",
  "theme": "cohesion",
  "output": "../",
  "urlFormat": "yyyy/MM/dd/slug",
  "directoryname": "",
  "pageSize" : "5",
  "copyDirectories": [
    "images",
    "javascripts",
    "stylesheets"
  ],
  "ignorables":[
    "cname",
    "compile.snow.bat",
    "snow.config",
    ".nojekyll",
    ".gitignore",
    ".deployment",
    ".git",
    ".svn",
    "svn",
    "snow",
    "readme.md"
  ],
  "defaultPostLayout": "post",
  "processFiles": [{
    "file": "index.cshtml",
    "loop": "Posts"
  },{
    "file": "category.cshtml",
    "loop": "Categories"
  },{
    "file": "categories.cshtml => category"
  },{
    "file": "archive.cshtml"
  },{
    "file": "about.cshtml"
  },{
    "file": "contact.cshtml"
  },{
    "file": "drafts.cshtml",
    "loop": "Drafts"
  },{
    "file": "feed.xml",
    "loop": "atom"
  }]
}
